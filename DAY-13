//1
#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int x,y;
	    cin>>x>>y;
	    
	    if(2*x==5*y)cout<<"Either"<<endl;
	   else if (2*x<5*y)cout<<"Candy"<<endl;
	    else cout<<"Choclate"<<endl;
	}
	return 0;
}
//2.
#include <iostream>
using namespace std;

int main() {
int t;
	cin>>t;
	while(t--)
	{
	    int x,y,z;
	    cin>>x>>y>>z;
	    
	    if(y<=z)cout<<x<<endl;
	    else {
	        if(y%z==0){
	        cout<<x*(y/z)<<endl;}
	        else{
	        cout<<x*(y/z+1)<<endl;}
	          }
	    
	}
	return 0;
}
//3
#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    for(int i=n;i>0;i--)
	    {cout<<i<<" ";}
	    cout<<endl;
	}
	return 0;
}

